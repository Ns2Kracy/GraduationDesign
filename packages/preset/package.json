{
    "name": "@gd/preset",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "types": "./dist/index.d.cts",
    "main": "./dist/index.cjs",
    "exports": {
        ".": {
            "source": "./src/index.ts",
