        "build": "tsup ./src/index.ts --dts",
        "lint": "oxlint && eslint"
    },
    "dependencies": {
        "@gd/eslint-config": "workspace:*"
    },
    "devDependencies": {
        "@pandacss/dev": "^0.27.3",
        "oxlint": "^0.2.8"
